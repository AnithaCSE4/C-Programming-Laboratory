#include<stdio.h>
int main()
{
    int n,m;
    scanf("%d%d",&n,&m);
    int arr1[n],arr2[m],arr3[m+n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr1[n]);
    }
    for(int i=0;i<m;i++)
    {
        scanf("%d",&arr2[m]);
        
    } 
    for(int i=0;i<n;i++)
    {
        arr3[i]=arr1[i];
    }
    for(int i=0;i<m;i++)
    {
        arr3[n+i]=arr2[i];
    }
    printf("merged array:\n");
    for(int i=0;i<n;i++)
    {
        printf("%d",arr3[i]);
    }
    return 0;
}

