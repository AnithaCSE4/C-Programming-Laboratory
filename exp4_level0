#include<stdio.h>
int main()
{
    int n,sum=0,i;
    {
        printf("enter the number of elements in array:\n");
    }
    scanf("%d",&n);
    int arr[n];
    {
        printf("enter the elements of array:\n");
    }
    for(i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i+=2)
    sum+=arr[i];
    {
        printf("enter the sum of element in array:%d",sum);
        
    }
    return 0;
}

