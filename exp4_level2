#include<stdio.h>
int main()
{
    int n,m;
    scanf("%d%d",&n,&m);
    int arr1[n],arr2[m],arr3[n+m];
    printf("enter %d elements for the first array:\n",n);
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr1[i]);
    }
    printf("enter %d elements for the second array:\n",m);
    for(int i=0;i<m;i++)
    {
        scanf("%d",&arr2[i]);
    }
    for(int i=0;i<n;i++)
    {
        arr3[i]=arr1[i];
    }
    for(int i=0;i<m;i++)
    {
        arr3[n=i]=arr2[i];
    }
    printf("merged array:\n");
    for(int i=0;i<n+m;i++)
    {
        printf("%d",arr3[i]);
    }
    
    return 0;
    
}
